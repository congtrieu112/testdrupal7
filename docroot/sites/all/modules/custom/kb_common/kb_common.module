<?php

/**
 * @file
 * Defines features and functions common to the K&B modules.
 */

/**
 * Implements of hook_form_alter().
 */
function kb_common_form_alter(&$form, &$form_state, $form_id) {
  switch ($form_id) {
    case 'programme_node_form':
      $form['field_idkp']['#access']=FALSE;
      $form['field_structure']['#access']=FALSE;

      break;

    default:
      break;
  }
}
